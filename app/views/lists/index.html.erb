<div class="banner" style="background-image: url('<%= asset_path('cinema.jpg') %>');">
  <div class="container">
    <h1>My Movie List</h1>
  </div>
</div>
<div class="container mt-3">
  <% @lists.each do |list| %>
    <%= link_to list_path(list) do %>
      <p><%= list.name %></p>
    <% end %>
  <% end %>
</div>
